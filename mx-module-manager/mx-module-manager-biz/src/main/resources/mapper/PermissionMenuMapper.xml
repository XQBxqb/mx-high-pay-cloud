<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.high.mx.module.manager.dao.PermissionMenuMapper">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, level, sort, permission_id, parent_id, key, name, icon
    </sql>
    <resultMap id="treeRoleMenuDto" type="cn.high.mx.module.manager.dto.TreeRoleMenuDTO">
        <result property="value" column="id" />
        <result property="parentId" column="parent_id" />
        <result property="level" column="level" />
        <result property="key" column="key" />
        <result property="title" column="icon" />
    </resultMap>
</mapper>
